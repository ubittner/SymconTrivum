{
  "elements": [
    {
      "type": "Image",
      "image": "data:image/png;base64,background-image:url('data:image/png;base64,"
    },
    {
      "type": "Label",
      "caption": "Trivum FlexLine"
    },
    {
      "type": "Label",
      "caption": " "
    },
    {
      "type": "Label",
      "caption": "This instance integrates Trivum FlexLine sound systems into IP-Symcon."
    },
    {
      "type": "Label",
      "caption": "For further information, please refer to the documentation or the notes here in the configuration."
    },
    {
      "type": "Label",
      "caption": " "
    },
    {
      "type": "ExpansionPanel",
      "caption": "(0) Instance information",
      "items": [
        {
          "type": "Label",
          "caption": "Symcon version:\t5.00>"
        },
        {
          "type": "Label",
          "caption": "Name:\t\t\tTrivum FlexLine"
        },
        {
          "type": "Label",
          "caption": "Version:\t\t\t1.04-3"
        },
        {
          "type": "Label",
          "caption": "Date:\t\t\tAugust 11, 2019"
        },
        {
          "type": "Label",
          "caption": "Time:\t\t\t18:00"
        },
        {
          "type": "Label",
          "caption": "Developer:\t\tUlrich Bittner"
        },
        {
          "type": "Label",
          "caption": "Project:\t\t\tA joint project of Normen Thiel and Ulrich Bittner"
        },
        {
          "type": "Label",
          "caption": "Prefix:\t\t\tUBTFL"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(1) General settings",
      "items": [
        {
          "type": "Label",
          "caption": "Please enter the IP address of the Trivum FlexLine device:"
        },
        {
          "type": "ValidationTextBox",
          "name": "DeviceIP",
          "caption": "IP address"
        },
        {
          "type": "Label",
          "caption": "Please specify the network timeout in milliseconds:"
        },
        {
          "type": "NumberSpinner",
          "name": "Timeout",
          "caption": "Timeout",
          "suffix": "ms"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(2) Output (Zone)",
      "items": [
        {
          "type": "Label",
          "caption": "Please specify the zone ID to be used as output:"
        },
        {
          "type": "NumberSpinner",
          "name": "ZoneID",
          "caption": "Zone ID"
        },
        {
          "type": "Label",
          "caption": "Please enter a zone name, e.g. room 2, zone 3 or dance hall:"
        },
        {
          "type": "ValidationTextBox",
          "name": "ZoneName",
          "caption": "Zone name"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(3) Inputs (Favorite list)",
      "items": [
        {
          "type": "Label",
          "caption": "The favorite list is used to control the inputs."
        },
        {
          "type": "Label",
          "caption": "If the input is LineIn, please activate the corresponding option."
        },
        {
          "type": "Label",
          "caption": "If you do not want to use a standard volume, please use the value -1."
        },
        {
          "type": "Label",
          "caption": " "
        },
        {
          "type": "Label",
          "caption": "Please add the favorites as inputs you want to use or delete entries that are not used anymore:"
        },
        {
          "type": "List",
          "name": "Favorites",
          "caption": "Favorites (Inputs)",
          "rowCount": 10,
          "add": true,
          "delete": true,
          "sort": {
            "column": "Position",
            "direction": "ascending"
          },
          "columns": [
            {
              "caption": "Position",
              "name": "Position",
              "width": "100px",
              "add": 1,
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "Favorite",
              "name": "Favorite",
              "width": "100px",
              "add": 1,
              "edit": {
                "type": "NumberSpinner"
              }
            },
            {
              "caption": "Description",
              "name": "Description",
              "width": "auto",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "name": "LineIn",
              "caption": "LineIn",
              "width": "150px",
              "add": false,
              "edit": {
                "type": "CheckBox"
              }
            },
            {
              "caption": "Standard volume in %",
              "name": "Volume",
              "width": "250px",
              "add": -1,
              "edit": {
                "type": "NumberSpinner"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(4) Multiroom (Members)",
      "items": [
        {
          "type": "Label",
          "caption": "A maximum of 15 zone members can be added."
        },
        {
          "type": "Label",
          "caption": "Please add the members you want to use for this zone or delete entries that are not used anymore:"
        },
        {
          "type": "List",
          "name": "ZoneMembers",
          "caption": "Zone members",
          "rowCount": 10,
          "add": true,
          "delete": true,
          "sort": {
            "column": "Position",
            "direction": "ascending"
          },
          "columns": [
            {
              "caption": "Position",
              "name": "Position",
              "width": "100px",
              "add": 1,
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "Zone ID",
              "name": "ZoneID",
              "width": "100px",
              "add": 0,
              "edit": {
                "type": "NumberSpinner"
              }
            },
            {
              "caption": "Description",
              "name": "Description",
              "width": "350px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "Instance ID",
              "name": "InstanceID",
              "width": "auto",
              "add": 0,
              "edit": {
                "type": "SelectInstance"
              }
            }
          ]
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "ExpansionPanel",
      "caption": "Action area",
      "items": [
        {
          "type": "Label",
          "caption": "(1) General settings"
        },
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "Button",
              "caption": "Show Favorites",
              "onClick": "UBTFL_ShowFavoriteList($id);"
            },
            {
              "type": "Button",
              "caption": "Enter Setup",
              "onClick": "UBTFL_SetupSystem($id);"
            },
            {
              "type": "Button",
              "caption": "Show WebUI",
              "onClick": "UBTFL_ShowWebUI($id);"
            }
          ]
        },
        {
          "type": "TestCenter"
        }
      ]
    },
    {
      "type": "Label",
      "caption": " "
    },
    {
      "type": "Button",
      "caption": "Manual",
      "onClick": "echo 'https://github.com/ubittner/SymconTrivum';"
    }
  ],
